{   
    "root": true
    "parser": "esprima",
    "parserOptions": {
    },
    "env": {
        "browser": true
    },
    "plugins": [

    ],
    "rules": {
        "eqeqeq": "warn",
        "no-cond-assign": "warn",
        "no-console": "warn",
        "no-constant-condition": "error",
        "no-control-regex": "warn",
        "no-debugger": "error",
        "no-dupe-args": "error",
        "no-dupe-keys": "error",
        "no-duplicate-case": "error"

    }
}